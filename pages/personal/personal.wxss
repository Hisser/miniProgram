/* pages/personal/personal.wxss */

.container {
  background-color: #f2f2f2;
  display: flex
}
.top {
  margin-bottom: 110px;
  display: flex;
  position: relative;
  flex-direction: column;
}

.userinfo {
  position: relative;
  background: #fd2c29;
  display: flex;
  flex-direction: row;
  height: 340rpx;
  background-image:url("data:image/png;base64,");
}

.userinfo-avatar {
  margin-right: 20rpx;
  margin-left: 20rpx;
  flex:2;
  text-align: center;
}

.username_view{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  
}
.vip_style{
  margin-left: 5px;
  width: 18px;
  height: 18px;
}
.userinfo-image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  border: 5rpx solid #fff;
  overflow: hidden;
}


.userinfo-nickname {
  color: #fff;
  font-size: 33rpx;
}

.userinfo-mobile{
  color: #fff;
  font-size: 28rpx;
}

.userinfo .userDetail{
  line-height: 36rpx;
  flex:4;
  font-size: 32rpx;
}
.userinfo .inviteInfo{
  margin-top: 0.5rem;
  display: flex;
  flex-direction: row;
}
.userinfo .inviteInfo text{
  font-size: 25rpx;
  color:#fff;
  border-radius:10% 
}

.userinfo .inviteInfo .copy{
  padding:2rpx 10rpx;
  background-color: #fff;
  color:#000;
  font-size: 25rpx;
  margin-left:10rpx;
  border-radius: 8rpx;
}

.userinfo .usertype-box{
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  margin: 0px 10px;
  line-height: 28px;
  flex:3;
  align-items:flex-end;
}
.userinfo .member{
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items:flex-start;
}

.bindPhone {
  position: relative;
  top: 40rpx;
  display: inline-block;
  background: rgba(255, 255, 255, 0.3);
  color: white;
  font-size: 24rpx;
  padding: 6rpx 20rpx 6rpx 20rpx;
  border-radius: 1000px;
}

.bindPhone image {
  width: 28rpx;
  height: 28rpx;
  padding-right: 10rpx;
  position: relative;
  bottom: 4rpx;
}

.w {
  color:#fd2c29;
  font-size:28rpx;
  background-color: #fff;
  border-radius: 20rpx;
  padding: 8rpx;
}
.withdraw{
  position: absolute; 
  top:220rpx;
  height: 190rpx;
  left: 19rpx;
  box-shadow: 0 1rpx 5rpx 0px rgba(0, 0, 0, 0.05);
  font-size: 30rpx;
  border-top: 1rpx solid #eee;
  border-radius: 5rpx;
  background: #fff;
}
.apply-vip-box.hidden {
  display: none;
}

.weui_cell {
  position: relative;
  display: flex;
  padding: 12px 15px 10px 15px;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  border-bottom: 1px solid #dadada;
}

.weui_cell_hd {
  display: inline-block;
  width: 20px;
  margin-right: 5px;
}

.weui_cell_hd image {
  width: 100%;
  height: 20px;
  vertical-align: -2px;
}

.weui_cell_bd {
  display: inline-block;
}

.weui_cell_bd_p {
  font-size: 14px;
  color: #737373;
}

.badge {
  position: absolute;
  top: 18px;
  right: 40px;
  width: 15px;
  height: 15px;
  line-height: 15px;
  background: #f00;
  color: #fff;
  border-radius: 50%;
  text-align: center;
  font-size: 8px;
}

.user_type, .apply-vip text {
  color: #f00;
  font-size: 28rpx;
  background: #fff;
  padding: 6rpx 24rpx;
  border-radius: 8rpx;
}

.invite_people {
  font-size: 28rpx;
  padding: 0rpx 12rpx 20rpx 0rpx;
  border-radius: 4px;
  color: #fff;
}

.user_information {
  padding-top: 20rpx;
  text-align: center;
  background: #fff;
  padding-bottom: 15rpx;
  border-top: 1px solid #e2e2e2;
  border-bottom: 1px solid #e2e2e2;
  display: flex
}
.cardInfo {
   flex-direction: column;
   margin: 18px auto;
   padding:18px 5px;
   width: 92%;
  }

.cardInfo .titleInfo{
  display: flex;
  flex-direction: row;
  justify-content: space-between ;
  height: 30px;
}

.cardInfo .joinVip{
  display: flex;
  flex-direction: row;
  justify-content: space-between ;
  align-items: center;
  text-align: center;
  height: 40px;
  background-color: rgba(240, 147, 105, 0.541);
  margin:10px 5px;
  padding:5px;
}
.cardInfo .joinVip text{
  font-size: 12px;
  color: rgb(71, 67, 55);
  text-align: center;
  padding: 3px;
}

.cardInfo .joinVip .upgrade{
  background-color: rgb(71, 67, 55);
  text-align: center;
}
.cardInfo .joinVip .upgrade text{
  font-size: 12px;
  color: #fd0;
}

.cardInfo .titleInfo .title{
  font-size: 14px;
  line-height: 20px;
  font-weight: bold
}

.cardInfo .boxInfo{
  display: flex;
  flex-direction: row;
  border-top: 1px solid #e2e2e2;
  padding:20px 15px
}
.user_information .inforbox {
  text-align: center;
  border-right: 1px solid #e2e2e2;
}

.user_information .inforbox:last-child {
  border-right: none;
}

.user_information .inforbox text {
  display: block;
  font-size: 14px;
  color: #737373;
}

.cardInfo .earnPic{
  width:100%;
  border-radius: 5px;
}

.info_list {
  background: #fff;
  margin-top: 10px;
  border-top: 1px solid #e2e2e2;
}

.ordinaryleader {
  color: #fff;
  font-size: 25rpx;
}

.tel-box {
  line-height: 1;
  padding: 0;
  background: none;
}
.footer {
  text-align: center;
  padding-bottom: 20px;
}
