}

.image-coupon {
  padding: 0 0 10rpx;
  box-sizing: border-box;
  height: 50rpx;
  width: 200rpx;
  vertical-align: middle;
  display: inline-block;
}

.price-coupon {
  color: #fff;
  font-size: 24rpx;
  padding: 0 0 10rpx;
  box-sizing: border-box;
  line-height: 50rpx;
  vertical-align: middle;
}

.image-circle {
  margin-right: 5rpx;
  height: 35rpx;
  width: 35rpx;
  vertical-align: middle;
}

#top-nav-bar-true text {
  border: 1rpx #ff495e solid;
  color: #ff495e;
}

#top-nav-bar-0 span {
  color: #444;
}

